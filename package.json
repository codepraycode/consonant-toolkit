{
  "name": "consonant-toolkit",
  "productName": "consonant-toolkit",
  "version": "1.0.5",
  "description": "A utility application for managing Consonant",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:windows": "electron-forge make --platform=win32",
    "make:mac": "electron-forge make --platform=darwin",
    "make:linux:snap": "electron-forge make --target=snap",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [
    "consonant",
    "codepraycode"
  ],
  "author": {
    "name": "codepraycode",
    "email": "preciousolusola16@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codepraycode/consonant-toolkit.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-dmg": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-snap": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-wix": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-vite": "^7.2.0",
    "@electron-forge/publisher-github": "^7.2.0",
    "@types/electron-prompt": "^1.6.5",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "28.2.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "sass": "^1.70.0",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "dotenv": "^16.4.1",
    "electron-log": "^5.1.0",
